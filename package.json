{
  "name": "phala-js-sdk",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach run build",
    "dev": "concurrently \"yarn dev:sdk\" \"yarn dev:ghost-auction\"",
    "dev:ghost-auction": "yarn workspace ghost-auction dev",
    "dev:sdk": "yarn workspace @phala/sdk dev",
    "lint": "yarn workspaces foreach run lint",
    "lint:fix": "yarn workspaces foreach run lint:fix",
    "publish": "npm publish ./packages/sdk --access public",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "concurrently": "^6.4.0",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "packageManager": "yarn@3.1.1"
}
